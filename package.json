{
  "name": "keys-translations-manager",
  "version": "1.6.0",
  "private": true,
  "description": "A web application for locale's keys and translations management.",
  "keywords": [
    "i18n",
    "internationalization",
    "lang",
    "language",
    "l10n",
    "locale",
    "localization",
    "multilingual",
    "translation",
    "MERN"
  ],
  "author": "chejen",
  "scripts": {
    "postinstall": "npm run build",
    "lint": "eslint --ext .js,.jsx src/**",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./Server.js",
    "start": "cross-env NODE_ENV=production babel-node ./Server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "lessc less/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
    "build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "cross-env NODE_ENV=test mocha -R spec --require @babel/register  --require ./tests/testHelper.js tests/**/*.js",
    "test:watch": "npm test -- --watch",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha tests/**/*.js",
    "codecov": "cat ./coverage/lcov.info | codecov"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chejen/keys-translations-manager"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "archiver": "^3.1.1",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "d3-hierarchy": "^1.1.8",
    "d3-selection": "^1.4.0",
    "d3-zoom": "^1.8.3",
    "ejs": "^2.7.1",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "isomorphic-fetch": "^2.2.1",
    "keys-translations-manager-core": "^1.6.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.9",
    "multiparty": "^4.2.1",
    "nodemon": "^1.19.4",
    "prop-types": "~15.7.2",
    "react": "~16.11.0",
    "react-bootstrap": "~0.32.4",
    "react-dom": "~16.11.0",
    "react-dropzone": "^10.1.10",
    "react-hot-loader": "^4.12.16",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-table": "^6.10.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/register": "^7.7.0",
    "@hot-loader/react-dom": "^16.11.0",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "clean-css-cli": "^4.3.0",
    "codecov": "^3.6.1",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-i18n": "^1.2.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eventsource-polyfill": "~0.9.6",
    "husky": "^3.0.9",
    "jsdom": "^15.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "mocha": "^6.0.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "react-test-renderer": "~16.11.0",
    "redux-mock-store": "^1.5.3",
    "rename": "^1.0.4",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-strip": "~0.1.0"
  },
  "license": "MIT"
}
