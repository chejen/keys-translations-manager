{
	"name": "keys-translations-manager",
	"version": "1.4.0",
	"private": true,
	"description": "A web application for locale's keys and translations management.",
	"keywords": [
		"i18n",
		"internationalization",
		"lang",
		"language",
		"l10n",
		"locale",
		"localization",
		"multilingual",
		"translation",
		"MERN"
	],
	"author": "chejen",
	"scripts": {
		"lint": "eslint \"src/**/*.js\"",
		"dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./Server.js",
		"start": "cross-env NODE_ENV=production babel-node ./Server.js",
		"build": "npm run build:css && npm run build:js",
		"build:css": "lessc less/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
		"build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
		"test": "cross-env NODE_ENV=test mocha -R spec --require babel-register  --require ./tests/testHelper.js tests/**/*.js",
		"test:watch": "npm test -- --watch",
		"coverage": "cross-env NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha tests/**/*.js --report lcovonly -- -R spec",
		"codecov": "cat ./coverage/lcov.info | codecov"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run test"
		}
	},
	"lint-staged": {
		"src/**/*.js": [
			"eslint"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chejen/keys-translations-manager"
	},
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
		"archiver": "^2.1.0",
		"body-parser": "^1.18.2",
		"bootstrap": "~3.3.7",
		"d3-hierarchy": "^1.1.5",
		"d3-selection": "^1.1.0",
		"d3-zoom": "^1.6.0",
		"es6-promise": "^4.1.1",
		"express": "^4.16.2",
		"isomorphic-fetch": "^2.2.1",
		"keys-translations-manager-core": "^1.4.1",
		"mongoose": "^5.0.0",
		"multiparty": "^4.1.3",
		"prop-types": "~15.6.0",
		"react": "~16.4.1",
		"react-bootstrap": "~0.32.1",
		"react-bootstrap-table": "~4.1.0",
		"react-dom": "~16.4.1",
		"react-dropzone": "^4.2.1",
		"react-redux": "^5.0.6",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"redux": "^4.0.0",
		"redux-thunk": "^2.2.0",
		"socket.io": "^2.0.3"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.0.1",
		"babel-istanbul": "~0.12.2",
		"babel-loader": "^7.1.2",
		"babel-plugin-add-module-exports": "~0.2.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.24.1",
		"chai": "^4.1.2",
		"clean-css-cli": "^4.1.10",
		"codecov": "^3.0.0",
		"compression": "^1.7.1",
		"cross-env": "^5.1.0",
		"css-loader": "^1.0.0",
		"ejs": "^2.5.7",
		"enzyme": "^3.1.0",
		"enzyme-adapter-react-16": "^1.0.2",
		"eslint": "^5.1.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-i18n": "^1.1.0",
		"eslint-plugin-react": "^7.4.0",
		"eventsource-polyfill": "~0.9.6",
		"husky": "^1.0.0-rc.13",
		"istanbul": "~0.4.5",
		"jsdom": "^11.3.0",
		"less": "^3.7.1",
		"less-loader": "^4.0.5",
		"lint-staged": "^7.2.0",
		"mocha": "^5.0.0",
		"nock": "^9.0.22",
		"nodemon": "^1.12.1",
		"react-test-renderer": "~16.4.1",
		"redux-mock-store": "^1.3.0",
		"rename": "^1.0.4",
		"serve-favicon": "^2.4.5",
		"sinon": "^6.1.3",
		"sinon-chai": "^3.2.0",
		"style-loader": "~0.21.0",
		"webpack": "^4.16.0",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-hot-middleware": "^2.20.0",
		"webpack-strip": "~0.1.0"
	},
	"license": "MIT"
}
