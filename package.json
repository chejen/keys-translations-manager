{
	"name": "keys-translations-manager",
	"version": "0.3.0",
	"private": true,
	"description": "A web application for locale's keys and translations management.",
	"keywords": [
		"i18n",
		"internationalization",
		"lang",
		"language",
		"l10n",
		"locale",
		"localization",
		"multilingual",
		"translation"
	],
	"author": "chejen",
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon --exec babel-node --ignore public/ --ignore src/ui/ -- ./Server.js",
		"start": "cross-env NODE_ENV=production babel-node ./Server.js",
		"build": "npm run build:css && npm run build:js",
		"build:css": "lessc src/ui/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
		"build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
		"test": "mocha -R spec --compilers js:babel-core/register test/**/*.js",
		"test:watch": "npm test -- --watch",
		"posttest": "babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha test/**/*.js --report lcovonly -- -R spec && codecov"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chejen/keys-translations-manager"
	},
	"engines": {
		"node": ">=5"
	},
	"dependencies": {
		"ag-grid": "4.0.5",
		"ag-grid-react": "4.0.0",
		"archiver": "0.21.0",
		"body-parser": "1.15.0",
		"bootstrap": "3.3.6",
		"es6-promise": "3.1.2",
		"express": "4.13.4",
		"isomorphic-fetch": "2.2.1",
		"keys-translations-manager-core": "0.1.0",
		"mongoose": "4.4.11",
		"react": "0.14.8",
		"react-addons-pure-render-mixin": "0.14.8",
		"react-bootstrap": "0.28.5",
		"react-dom": "0.14.8",
		"react-redux": "4.4.1",
		"redux": "3.3.1",
		"redux-thunk": "2.0.1"
	},
	"devDependencies": {
		"babel-cli": "6.6.5",
		"babel-core": "6.7.4",
		"babel-eslint": "6.0.2",
		"babel-istanbul": "0.7.0",
		"babel-loader": "6.2.4",
		"babel-preset-es2015": "6.6.0",
		"babel-preset-react": "6.5.0",
		"babel-preset-react-hmre": "1.1.1",
		"babel-preset-stage-0": "6.5.0",
		"chai": "3.5.0",
		"clean-css": "3.4.11",
		"codecov": "1.0.1",
		"compression": "1.6.1",
		"cross-env": "1.0.7",
		"css-loader": "0.23.1",
		"ejs": "2.4.1",
		"eslint": "2.6.0",
		"eslint-loader": "1.3.0",
		"eslint-plugin-babel": "3.1.0",
		"eslint-plugin-i18n": "1.0.1",
		"eslint-plugin-react": "4.2.3",
		"eventsource-polyfill": "0.9.6",
		"istanbul": "0.4.2",
		"less": "2.6.1",
		"less-loader": "2.2.3",
		"mocha": "2.4.5",
		"nock": "7.7.2",
		"nodemon": "1.9.1",
		"redux-mock-store": "1.0.2",
		"rename": "1.0.3",
		"serve-favicon": "2.3.0",
		"sinon": "1.17.3",
		"sinon-chai": "2.8.0",
		"style-loader": "0.13.1",
		"webpack": "1.12.14",
		"webpack-dev-middleware": "1.6.1",
		"webpack-hot-middleware": "2.10.0",
		"webpack-strip": "0.1.0"
	},
	"license": "MIT"
}
