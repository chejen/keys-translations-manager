{
	"name": "keys-translations-manager",
	"version": "1.4.0",
	"private": true,
	"description": "A web application for locale's keys and translations management.",
	"keywords": [
		"i18n",
		"internationalization",
		"lang",
		"language",
		"l10n",
		"locale",
		"localization",
		"multilingual",
		"translation",
		"MERN"
	],
	"author": "chejen",
	"scripts": {
		"postinstall": "npm run build",
		"lint": "eslint --ext .js,.jsx src/**",
		"dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./Server.js",
		"start": "cross-env NODE_ENV=production babel-node ./Server.js",
		"build": "npm run build:css && npm run build:js",
		"build:css": "lessc less/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
		"build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
		"test": "cross-env NODE_ENV=test mocha -R spec --require babel-register  --require ./tests/testHelper.js tests/**/*.js",
		"test:watch": "npm test -- --watch",
		"coverage": "cross-env NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha tests/**/*.js --report lcovonly -- -R spec",
		"codecov": "cat ./coverage/lcov.info | codecov"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run test"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx}": [
			"eslint"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chejen/keys-translations-manager"
	},
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
		"archiver": "^3.0.0",
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"body-parser": "^1.18.3",
		"compression": "^1.7.3",
		"cross-env": "^5.2.0",
		"d3-hierarchy": "^1.1.8",
		"d3-selection": "^1.3.2",
		"d3-zoom": "^1.7.3",
		"ejs": "^2.6.1",
		"es6-promise": "^4.2.5",
		"express": "^4.16.3",
		"history": "^4.7.2",
		"isomorphic-fetch": "^2.2.1",
		"keys-translations-manager-core": "^1.5.0",
		"lodash": "^4.17.11",
		"mongoose": "^5.2.17",
		"multiparty": "^4.2.1",
		"nodemon": "^1.18.4",
		"prop-types": "~15.6.2",
		"react": "~16.5.2",
		"react-bootstrap": "~0.32.4",
		"react-dom": "~16.5.2",
		"react-dropzone": "^5.1.0",
		"react-hot-loader": "^4.3.11",
		"react-redux": "^5.0.7",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-table": "^6.8.6",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0",
		"serve-favicon": "^2.5.0",
		"socket.io": "^2.1.1",
		"socket.io-client": "^2.1.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-istanbul": "~0.12.2",
		"babel-loader": "^7.1.5",
		"babel-plugin-add-module-exports": "~0.2.1",
		"chai": "^4.1.2",
		"clean-css-cli": "^4.2.0",
		"codecov": "^3.1.0",
		"css-loader": "^0.28.7",
		"enzyme": "^3.6.0",
		"enzyme-adapter-react-16": "^1.5.0",
		"eslint": "^5.6.0",
		"eslint-loader": "^2.1.1",
		"eslint-plugin-babel": "^5.2.0",
		"eslint-plugin-i18n": "^1.2.1",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-react": "^7.11.1",
		"eventsource-polyfill": "~0.9.6",
		"husky": "^1.0.0-rc.13",
		"istanbul": "~0.4.5",
		"jsdom": "^11.12.0",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"lint-staged": "^7.3.0",
		"mocha": "^5.2.0",
		"nock": "^9.4.4",
		"react-test-renderer": "~16.5.2",
		"redux-mock-store": "^1.5.3",
		"rename": "^1.0.4",
		"sinon": "^6.3.4",
		"sinon-chai": "^3.2.0",
		"style-loader": "~0.21.0",
		"webpack": "^3.8.1",
		"webpack-dev-middleware": "^1.12.0",
		"webpack-hot-middleware": "^2.20.0",
		"webpack-strip": "~0.1.0"
	},
	"license": "MIT"
}
