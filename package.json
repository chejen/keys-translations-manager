{
	"name": "keys-translations-manager",
	"version": "1.0.0",
	"private": true,
	"description": "A web application for locale's keys and translations management.",
	"keywords": [
		"i18n",
		"internationalization",
		"lang",
		"language",
		"l10n",
		"locale",
		"localization",
		"multilingual",
		"translation"
	],
	"author": "chejen",
	"scripts": {
		"postinstall": "npm run build",
		"dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./Server.js",
		"start": "cross-env NODE_ENV=production babel-node ./Server.js",
		"build": "npm run build:css && npm run build:js",
		"build:css": "lessc less/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
		"build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
		"test": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register  --require ./tests/testHelper.js tests/**/*.js",
		"test:watch": "npm test -- --watch",
		"posttest": "cross-env NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha tests/**/*.js --report lcovonly -- -R spec && codecov"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chejen/keys-translations-manager"
	},
	"engines": {
		"node": ">=5"
	},
	"dependencies": {
		"ag-grid": "4.2.5",
		"ag-grid-react": "4.2.0",
		"archiver": "^1.0.0",
		"body-parser": "^1.15.1",
		"bootstrap": "~3.3.6",
		"es6-promise": "^3.2.1",
		"express": "^4.13.4",
		"isomorphic-fetch": "^2.2.1",
		"keys-translations-manager-core": "^1.1.0",
		"mongoose": "^4.4.19",
		"multiparty": "^4.1.2",
		"react": "~15.1.0",
		"react-addons-pure-render-mixin": "~15.1.0",
		"react-bootstrap": "~0.29.4",
		"react-dom": "~15.1.0",
		"react-dropzone": "^3.4.0",
		"react-redux": "^4.4.5",
		"redux": "^3.5.2",
		"redux-thunk": "^2.1.0"
	},
	"devDependencies": {
		"babel-cli": "^6.9.0",
		"babel-core": "^6.9.1",
		"babel-eslint": "^6.0.4",
		"babel-istanbul": "~0.8.0",
		"babel-loader": "^6.2.4",
		"babel-plugin-add-module-exports": "~0.2.1",
		"babel-preset-es2015": "^6.9.0",
		"babel-preset-react": "^6.5.0",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.5.0",
		"chai": "^3.5.0",
		"clean-css": "^3.4.13",
		"codecov": "^1.0.1",
		"compression": "^1.6.2",
		"cross-env": "^1.0.8",
		"css-loader": "^0.23.1",
		"ejs": "^2.4.2",
		"enzyme": "^2.3.0",
		"eslint": "^2.11.0",
		"eslint-loader": "^1.3.0",
		"eslint-plugin-babel": "^3.2.0",
		"eslint-plugin-i18n": "^1.0.1",
		"eslint-plugin-react": "^5.1.1",
		"eventsource-polyfill": "~0.9.6",
		"istanbul": "~0.4.3",
		"jsdom": "^9.2.1",
		"less": "^2.7.1",
		"less-loader": "^2.2.3",
		"mocha": "^2.5.3",
		"nock": "^8.0.0",
		"nodemon": "^1.9.2",
		"react-addons-test-utils": "~15.1.0",
		"redux-mock-store": "^1.0.4",
		"rename": "^1.0.3",
		"serve-favicon": "^2.3.0",
		"sinon": "^1.17.4",
		"sinon-chai": "^2.8.0",
		"style-loader": "~0.13.1",
		"webpack": "^1.13.1",
		"webpack-dev-middleware": "^1.6.1",
		"webpack-hot-middleware": "^2.10.0",
		"webpack-strip": "~0.1.0"
	},
	"license": "MIT"
}
